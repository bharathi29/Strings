import java.util.Scanner;
public class Nine {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter string :");
        String s=sc.next();
        s=s.replaceAll("[aeiouAEIOU]","");
        System.out.println("After removing vowels: "+s);
    }
}
